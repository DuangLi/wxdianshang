/* components/searchBar/searchBar.wxss */
.search_bar{
  width: 100%;
  height: 90rpx;
  padding: 20rpx;
  background-color: var(--themeColor);
}

.search_bar navigator{
  display: flex;
  height: 60rpx;
  border-radius: 20rpx;
  justify-content: center;
  align-items: center;
  background-color: white;
}

.search_bar navigator icon{
  padding-right: 5rpx;
}